{
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc"]
    },
    "source": {
        "include": ["lib", "README.md"],
        "includePattern": ".js$",
        "excludePattern": "(node_modules|test|public|docs|dist|.cache)"
    },
    "plugins": [
        "plugins/markdown"
    ],
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": true,
        "useLongnameInNav": false,
        "showInheritedInNav": true
    },
    "opts": {
        "destination": "./jsdoc/",
        "encoding": "utf8",
        "private": true,
        "recurse": true
    }
}
